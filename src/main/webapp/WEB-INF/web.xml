<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <listener>
         <listener-class>com.sinensia.controllers.MyListenerController</listener-class>
	</listener>
    <servlet>
    	<servlet-name>CategoriaController</servlet-name>
    	<display-name>CategoriaController</display-name>
    	<description></description>
    	<servlet-class>com.sinensia.controllers.CategoriaController</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>CategoriaControllers</servlet-name>
    	<display-name>CategoriaControllers</display-name>
    	<description></description>
    	<servlet-class>com.sinensia.controllers.CategoriaController</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>CategoriaController</servlet-name>
    	<url-pattern>/CategoriaController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>CategoriaControllers</servlet-name>
    	<url-pattern>/CategoriaControllers</url-pattern>
    </servlet-mapping>
</web-app>
